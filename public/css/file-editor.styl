perspective(val)
	-webkit-perspective val
	-moz-perspective val
	-ms-perspective val
	-o-perspective val
	perspective val

transform-style(val)
	-webkit-transform-style val
	-moz-transform-style val
	-ms-transform-style val
	-o-transform-style val
	transform-style val

backface-visibility(val)
	-webkit-backface-visibility val
	-moz-backface-visibility val
	-ms-backface-visibility val
	-o-backface-visibility val
	backface-visibility val

transform(val)
	-webkit-transform val
	-moz-transform val
	-ms-transform val
	-o-transform val
	transform val

selection(val)
	-webkit-touch-callout none
	-webkit-user-select none
	-khtml-user-select none
	-moz-user-select none
	-ms-user-select none
	user-select none

@font-face
	font-family "mono"
	src url("../fonts/Inconsolata.otf")

body
	position fixed
	top 0
	bottom 0
	left 0
	right 0

/* =FileEditor
*************************************************/
.file-editor
	position absolute
	width w = 100%
	height 100%
	box-sizing border-box
	font-family sans-serif
	padding-top 1.8em
	padding-bottom 1.5em
	selection none
	-webkit-transition padding 0.5s
	-moz-transition padding 0.5s
	-ms-transition padding 0.5s
	-o-transition padding 0.5s
	transition padding 0.5s

	.fullscreen
		height 1em
		float right
		padding 0.4em
		padding-bottom 0
	.file-slider
		position absolute
		top 0
		width 100%
		selection none
		background #373832
		border-bottom 1px solid #555

		.slide
			white-space nowrap
			overflow hidden

		.entry
			display inline-block
			padding 0 0.75em
			font-size 0.8em
			margin 0 0.1em
			cursor pointer
			color #666
			line-height 2.2em
			max-width 500px
		
			overflow hidden
			-webkit-transition margin 0.5s, padding 0.3s, max-width 0.3s, opacity 0.3s
			-moz-transition margin 0.5s, padding 0.3s, max-width 0.3s, opacity 0.3s
			-ms-transition margin 0.5s, padding 0.3s, max-width 0.3s, opacity 0.3s
			-o-transition margin 0.5s, padding 0.3s, max-width 0.3s, opacity 0.3s
			transition margin 0.5s, padding 0.3s, max-width 0.3s, opacity 0.3s

			span
				color #555
				font-size 0.8em
				font-weight bold
				padding 0 0.5em
				display inline-block
				margin-right -0.5em
				margin-left 0.2em

		.entry.selected,
		.entry.open
			color #ccc
		.entry.selected
			background #272822
			border-radius 0.5em
			line-height 1.8em
			box-shadow inset 0 2px 5px rgba(0,0,0,0.6)
		.entry.spacer
			opacity 0
			padding 0
		.entry.moving
			position absolute
			z-index 5
		.entry.closing
			-webkit-transition left 0.3s
			-moz-transition left 0.3s
			-ms-transition left 0.3s
			-o-transition left 0.3s
			transition left 0.3s
			
		.entry.removed
			max-width 0
			padding 0.2em 0
			margin 0
			opacity 0

	.wrap
		position relative
		height 100%
		overflow hidden

	.settings,
	.editor
		height 100%
		display block
		width 100%
		position absolute
		box-sizing border-box
		z-index 4
	
	.file-browser
		height 100%
		display block
		width 100%
		position absolute
		box-sizing border-box
		z-index 8
		background rgba(39,40,34,0.9)
		border-radius 0.8em
		top 0
		left 0
		visibility hidden
		opacity 0

		-webkit-transition opacity 0.5s, visibility 0s linear 0.5s
		-moz-transition opacity 0.5s, visibility 0s linear 0.5s
		-ms-transition opacity 0.5s, visibility 0s linear 0.5s
		-o-transition opacity 0.5s, visibility 0s linear 0.5s
		transition opacity 0.5s, visibility 0s linear 0.5s

		.login
			width 25%
			float right
			margin 0 1em
			height 100%
			position relative
			.prompt
				position absolute
				bottom 1em
				text-align center
			.title
				font-size 1.8em
				margin-bottom 0.4em
				color #fff
				div
					line-height 0.8em
					color #888
					font-size 0.5em
				span
					color #555
					display inline-block
					padding 0 0.5em
					cursor pointer
			.row
				margin 0 1em
				.title
					float left
					width 4.5em
					text-align right
					font-size 0.9em
					margin-top 0.4em
					margin-right 0.5em
					margin-bottom 0
					color #555
			.input-wrap
				overflow hidden
				input
					border none
					border-radius 0.2em
					padding 0.2em
					width 100%
					font-family inherit
					font-size inherit
					box-sizing border-box
					outline none
					outline-color rgba(0,0,0,0)
			.button
				display inline-block
				color #888
				font-size 0.8em
				cursor pointer
				vertical-align middle
				padding-top 0.5em
			.button.main
				color #fff
				font-size 1.3em
				margin 0 0.4em
				margin-top 0.4em
				padding-top 0
		.login.error
			.title
				div
					color #f00
		.info
			height 100%
			position relative
			box-sizing border-box
			width 25%
			float right
			opacity 1
			display none

			-webkit-transition opacity 0.5s
			-moz-transition opacity 0.5s
			-ms-transition opacity 0.5s
			-o-transition opacity 0.5s
			transition opacity 0.5s
			
			.display.blank
				position absolute
				top 50%
				width 100%
				margin-top -1em
				text-align center

				h1
					margin 0
					color #e8e8ef
					font-size 1.6em
					line-height 1em
				h2
					margin 0
					color #666
					font-size 1em
					line-height 1em
			.display.file,
			.display.folder
				position absolute
				top 50%
				margin-top -1.8em
				display none
				img
					width 4em
					position absolute
					bottom 0.2em
				a.download
					position absolute
					bottom -0.4em
					font-size 0.6em
					color #ccc
					width 6.8em
					text-align center
					left 0
					cursor pointer
				.name
					color #fff
					font-size 1.4em
					overflow hidden
					white-space nowrap
					margin-left 3em
				.size
					margin-left 5.2em
					color #888
					font-size 0.8em
				.modified
					font-size 0.8em
					margin-left 5em
					color #888
					white-space nowrap
					span
						color #ccc
			.display.folder
				margin-top -1em
			.display.create
				position absolute
				top 50%
				width 100%
				box-sizing border-box
				margin-top -1.8em
				padding 0 1em 2.3em
				display none
				input
					font-family inherit
					font-size inherit
					color inherit
					background #fff
					width 100%
					padding 0.2em 0.5em
					font-size 1.2em
					box-sizing border-box
					border-radius 0.5em
					border none
					box-shadow inset 0 1px 5px rgba(0,0,0,0.3)
				.option
					float left
					width 40%
					margin 0 5%
					padding 0.2em 0 0.5em
					color #fff
					text-align center
					cursor pointer
				.option.disabled
					color #555
				
			.action
				width 100%
				position absolute
				bottom 0
				border-top 1px solid #333
				
				.button
					float left
					width 40%
					margin 0 5%
					text-align center
					color #fff
					padding 0.5em 0
					cursor pointer
					border-right 1px solid #333
				.button.last
					border-right none
				.button.disabled
					color #333
			.action.top
				top 0
				bottom auto
				border none
				border-bottom 1px solid #333

		.pane-slide
			height 100%
			box-sizing border-box
			overflow-x auto
			overflow-y hidden
			white-space nowrap
			margin 0 0.5em
			opacity 1
			display none

			-webkit-transition opacity 0.5s
			-moz-transition opacity 0.5s
			-ms-transition opacity 0.5s
			-o-transition opacity 0.5s
			transition opacity 0.5s

			.pane
				height 100%
				box-sizing border-box
				padding 0.5em 0
				width 30%
				overflow-x hidden
				overflow-y auto
				border-right 2px solid #333
				display inline-block
				vertical-align top

				-webkit-transition width 0.8s
				-moz-transition width 0.8s
				-ms-transition width 0.8s
				-o-transition width 0.8s
				transition width 0.8s
				
				.entry
					padding 0.2em 0 0 1.9em
					color #e8e8ef
					cursor pointer
					background-position 0.5em 50%
					background-size 1.2em
					background-repeat no-repeat
				.entry.selected
					color #fff
					background-color #00bfff
				.entry.folder
					background-image url(../images/folder.png)
				.entry.folder.selected
					background-image url(../images/folder_open.png)
				.entry.file
					background-image url(../images/file.png)
				.entry.file.unsupported
					opacity 0.2
				.entry.file.unsupported.selected
					color #333
					background-color #fff
				.entry.new
					background rgba(255,255,255,0.05)
					margin-top 0.5em
					span
						float left
						position relative
						font-weight bold
						margin-left -1em
						margin-right 0.45em
						color #00bfff
				.entry.new.selected
					background-color #00bfff
					span
						color #fff

	.editor
		box-shadow inset 0 3px 8px rgba(0,0,0,0.4)

	.settings
		font-family mono
		z-index 6
		background #272822
		color #fff
		height 7em
		box-shadow 0 0 10px rgba(0,0,0,0.6)
		bottom -26%

		-webkit-transition bottom 0.3s
		-moz-transition bottom 0.3s
		-o-transition bottom 0.3s
		transition bottom 0.3s

		.menu
			text-align center
			width 16em
			padding 1em
			float left
	
			h1
				margin 0 0 0.3em
				border-bottom 1px solid #404040
				background transparent
				font-size 1.5em
				text-align left
	
				span
					color #fff

			select
				font-size 1em	
			input
				border 2px solid #eee
				background #eee
				border-radius 0.3em
				font-size 1em
				outline none
				text-align right
				font-family inherit
				width 6em
			
				-webkit-transition border-color 0.3s
				-moz-transition border-color 0.3s
				-ms-transition border-color 0.3s
				-o-transition border-color 0.3s
				transition border-color 0.3s
	
			input.invalid
				border 2px solid #c00
		.disclaimer
			overflow hidden
			text-align center
			font-size 2em
			width 19em
			color #373832
			margin 0.5em auto

	.bottom-wrap
		position absolute
		width 100%
		height 100%
		box-sizing border-box
		padding-top 3em
		top 0
		overflow hidden

	.footer
		position absolute
		width 100%
		height 1.5em
		bottom 0
		box-sizing border-box
		padding 0 0.5em
		text-align center
		background #373832
		border-top 1px solid #555
		box-shadow 0 0 8px rgba(0,0,0,0.6)
	
		.line-count
			float left
			font-size 0.8em
			color #ccc
			padding-top 0.3em
	
			span
				margin-right 1em
				margin-left 0.5em
				display inline-block
	
			input
				font-family inherit
				font-size inherit
				text-align right
				width 2.2em
				background rgba(0,0,0,0.05)
				color #fff
				border none
				margin 0
				padding 0
				margin-right 1em
				margin-left 0.5em
				outline none
		
			input::-moz-selection,
			input::selection
				background #ffb46c
		
		.set-button
			float right
			font-size 0.8em
			color #888
			padding 0.3em 0.5em 0
			margin-right 1em
			cursor pointer

		select
			float right

.file-editor.edit
	.settings
		bottom 0
	.footer
		.set-button
			color #fff

.file-browser.open
	visibility visible
	opacity 1
	-webkit-transition-delay 0
	-moz-transition-delay 0
	-ms-transition-delay 0
	-o-transition-delay 0
	transition-delay 0
